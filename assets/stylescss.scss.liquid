/*============================================================================
  #headings / text /Buttons
==============================================================================*/

.button{
  cursor:pointer;
  text-align:center;
  cursor:poitner;
  -webkit-appearance:none;
  font-family:inherit;
  border-radius:0;
  outline:none;
  display:inline-block;
  border:none;
  outline:none;
  @include webkitButtonDisplay();
  background:$accent;
  border:1px solid $accent;
  color:#fff;
  text-transform:uppercase;
  letter-spacing:2px;
  line-height:1.2;
  padding:21px 25px;
  font-size:1.4rem;
  font-weight:700;
  @include transition (all 200ms linear);
  &:hover{
    background:$accentHover;
    border-color:$accentHover;
  }
  
  &.disabled, &.disabled:hover{
    background:#444;
    border-color:#444;
    cursor:default;
  }
  
  &.borderAccent{
    color:$accent;
    border-color:$accent;
    background:transparent;
    &:hover{
      border-color:$accentHover;
      color:$accentHover;
    }
  }
  &.black{
    background:#000;
    border-color:#000;
    &:hover{
      background:#888;
      border-color:#888;
    }
  }
  &.lineLink{
    color:$accent;
    border:none;
    background:transparent;
    font-weight: 300;
    font-size: 1.8rem;
    text-transform:none;
    letter-spacing:0;
    text-decoration:underline;
    padding: 0;
    line-height:3rem;
    height:auto;
    &:hover{
      border-color:$accentHover;
      color:$accentHover;
    }
  }
  &.small{
    padding:0 10px;
    line-height:48px;
    font-size:1.3rem;
  }
}

