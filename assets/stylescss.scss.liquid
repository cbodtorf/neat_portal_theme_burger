/*============================================================================
  #header / nav
==============================================================================*/


main{
  background:#fff;
  min-height:50vh;
}
$headerHeight:80px;
header{
  position:relative;
  width:100%;
  color:#222;
  @include transition (color 300ms linear, border 300ms linear);
  border-bottom:1px solid #eee;
  height:81px;
  z-index:999;
  &:after{
    position:absolute;
    top:0;
    left:0;
    height:$headerHeight;
    z-index:-1;
    width:100%;
    height:100%;
    background:#fff;
    content:'';
    @include transition (opacity 300ms linear);
    z-index:-1;
  }
  .grid{
   position:relative;
  }
  a{
    line-height:$headerHeight;
    display:block;
  }
  .burger{
    display:none;
   }
  
  
  .logo{
    width:200px;
    height:$headerHeight;
    position:absolute;
    left:0;
    top:0;
    &:before{
      content:'';
      position:absolute;
      top:0;
      left:0;
      width:100%;
      height:100%;
      background-size:auto 42px;
      background-position:center;
      background-repeat:no-repeat;
      background-image:url({{'logo_w.png'|asset_url}});     
      opacity:0;
      @include transition (opacity 300ms linear);
    }
    &:after{
      content:'';
      position:absolute;
      top:0;
      left:0;
      width:100%;
      height:100%;
      background-size:auto 42px;
      background-position:center;
      background-repeat:no-repeat;
      background-image:url({{'logo_b.png'|asset_url}});     
      opacity:1;
      @include transition (opacity 300ms linear);
    }
  }
  
  nav {
    a.parentLink{
      text-align:center;
      font-size:1.4rem;
      letter-spacing:2px;
      padding:0 20px;
      text-transform:uppercase;
      position:relative;
      &[href="/account"]{
       display:none;
      }
    }
    
    .linkWrap{
      display:block;
      a.parentLink{
        &:before{
          position:absolute;
          bottom:-1px;
          left:0;
          width:100%;
          height:5px;
          content:'';
          background:$accent;
          opacity:0;
        }
      }
      &.active,&:hover{
        a.parentLink{
          &:before{
            opacity:1;
          }
        }
      }
      
      &.active{
        .dropdownMenu {
          @include prefixer(transform,translateY(0%),ms webkit spec);
          opacity:1;
        }
        .hasDrop:after{
          @include prefixer(transform,rotate(45deg),ms webkit spec);
          @include prefixer(transform-origin,center,ms webkit spec);
        }
      }
    }
    .hasDrop{
      &:after{
