form,.formProxy{
  font-size:1.6rem;
  input,select,textarea{
    font-size:inherit;
    font-family:inherit;
    color:inherit;
  }
  input,textarea{
    &[type="text"],&[type="search"],&[type="email"],&[type="number"],&[type="password"],&[type="tel"]{
      line-height:58px;
      border:1px solid #eee;
      border-radius:0;
      outline:none;
      width:100%;
      outline:none;
      margin-bottom:10px;
      &:not([type="number"]){
        -webkit-appearance:none;
      }
      &:not([type="number"]){
        padding:0 20px;
      }
      &[type="number"]{
        padding:0 0 0 10px;
      }
    }
  }
  input[type=number]::-webkit-inner-spin-button, 
  input[type=number]::-webkit-outer-spin-button{
    -webkit-appearance: spinner; 
  }
  select{
    height:50px;
    padding:0 0 0 10px;
    border:1px solid #eee;
    width:100%;
    outline:none;
  }
  textarea{
    padding:20px;
    border:1px solid #eee;
    border-radius:0;
    outline:none;
    width:100%;
    outline:none;
    margin-bottom:10px;
    -webkit-appearance:none;
  }
  label{
    text-align:left;
    margin-bottom:10px;
    color:#777;
    display:block;
    font-size:1rem;
    font-weight:500;
    text-transform:uppercase;
    letter-spacing:2px;
  }

  
