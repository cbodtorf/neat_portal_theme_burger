/*============================================================================
  #animate
==============================================================================*/
.fadeInLoad{
  //opacity:0;
}

.fadeIn{
  opacity:0;
  @include transition (opacity 800ms linear,transform 800ms linear);
  @include prefixer(transform,translate3d(0%,20px,0),ms webkit spec);
  @include prefixer(transform,translate(0%,20px),ms webkit spec);
  &.loaded{
    opacity:1;
    @include prefixer(transform,translate3d(0%,0px,0),ms webkit spec);
    @include prefixer(transform,translate(0%,0px),ms webkit spec);
    &.topScroll{
      opacity:0;
      @include prefixer(transform,translate3d(0%,-48px,0),ms webkit spec);
      @include prefixer(transform,translate(0%,-48px),ms webkit spec);
    }
  }
}

